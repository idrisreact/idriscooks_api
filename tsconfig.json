{
    "compilerOptions": {
      "target": "ES2020",          
      "module": "commonjs",        
      "strict": true,              
      "esModuleInterop": true,     
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
      "rootDir": "src",
      "outDir": "dist",
      "moduleResolution": "node"
    },
    "include": ["src/**/*.ts"]
  }